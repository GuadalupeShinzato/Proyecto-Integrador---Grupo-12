	<!-- /Head -->
	<%- include('./partials/head')%>
	<!-- /Head -->

	<body>
		<!-- /Header -->
		<% if (logueado) { %>
		<%- include('./partials/headerLogueado')%>
		<% } else { %>
		<%- include('./partials/header')%>
		<% } %>

		<!-- /Header -->

		<!-- Novedades -->
		<div class="container products-wrapper">
			<div class="row">
				<div class="col-12">
					<h2 class="products-title">Registrate</h2>
				</div>
				<!-- Show the "ultimos" products -->
				<div class="formulario">
					<form action="/register" style="font-size: small;" method="POST" enctype="multipart/form-data">
						Nombre: <input class="form-input" type="text" name="nombre" id="" required>
						Apellido: <input class="form-input" type="text" name="apellido" id="" required>
						E-mail: <input class="form-input" type="email" name="email" id="" required>
						Fecha de nacimiento: <input class="form-input" type="date" name="fecha" id="" required>
						Nombre de usuario: <input class="form-input" type="text" name="usuario" id="" required>
						Foto de perfil: <input class="form-input" type="file" name="portada" id=""
							accept="image/png, image/jpg, image/jpeg">
						Contraseña: <input class="form-input" type="password" name="contraseña" id="" required>
						Confirmar contraseña: <input class="form-input" type="password" name="confirContra" id="" required>
						<% if (error) { %>
						<h4 style="color: red;"> <%= error %> </h4>
						<% } %>
						<input class="form-input" type="submit" value="Registrate">
					</form>
				</div>

			</div>
		</div>
		<!-- footer -->
		<%- include('./partials/footer')%>
		<!-- /footer -->
	</body>

	</html>
	<!-- /Head -->
	<%- include('./partials/head')%>
	<!-- /Head -->

	<body>
		<!-- /Header -->
		<% if (logueado) { %>
			<%- include('./partials/headerLogueado')%>
		<% } else { %>
		<%- include('./partials/header')%>
		<% } %> 
		
		<!-- /Header -->


		<!-- Novedades -->
		<div class="container products-wrapper">
			<div class="row">
				<div class="col-12">
					<h2 class="products-title">Resultados de b√∫squeda</h2>
				</div>
				<!-- Show the "ultimos" products -->
				<% for( let i = 0; i < productos.length; i++ ) { %>
					<div class="col-12 col-sm-6 col-lg-3">
						<section class="product-box">
							<a href="product/id/<%= productos[i].id %> ">
								<figure class="product-box_image">
									<img src="/images/products/<%= productos[i].cover %> " alt="<%= productos[i].title %> ">
								</figure>
								<article class="product-box_data">
									<h2><%= productos[i].title %></h2>
									<p><%= productos[i].author %></p>
									<p>Comentarios: 12</p>
								</article>
							</a>
						</section>
					</div>
				<% } %>	
				</div>
				


				<!-- footer -->
				<%- include('./partials/footer')%>
				<!-- /footer -->
	</body>

	</html>
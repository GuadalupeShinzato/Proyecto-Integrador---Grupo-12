	<!-- /Head -->
	<%- include('./partials/head')%>
	<!-- /Head -->

	<body>
		<!-- /Header -->
		<% if (logueado) { %>
		<%- include('./partials/headerLogueado')%>
		<% } else { %>
		<%- include('./partials/header')%>
		<% } %>

		<!-- /Header -->

		<!-- Banner -->

		<div class="container mt-5">
			<div class="row">
				<a href="/">
					<div id="carouselExampleFade" class="carousel slide carousel-fade" data-bs-ride="carousel">
						<div class="carousel-inner">
							<div class="carousel-item active">
								<img src="/images/carr/ban.jpeg" class="d-block w-100" alt="...">
							</div>
							<div class="carousel-item">
								<img src="/images/carr/car1.jpeg" class="d-block w-100" alt="...">
							</div>
							<div class="carousel-item">
								<img src="/images/carr/carr2.jpeg" class="d-block w-100" alt="...">
							</div>
							<div class="carousel-item">
								<img src="/images/carr/car3.jpeg" class="d-block w-100" alt="...">
							</div>
						</div>
						<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade"
							data-bs-slide="prev">
							<span class="carousel-control-prev-icon" aria-hidden="true"></span>
							<span class="visually-hidden">Previous</span>
						</button>
						<button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade"
							data-bs-slide="next">
							<span class="carousel-control-next-icon" aria-hidden="true"></span>
							<span class="visually-hidden">Next</span>
						</button>
					</div>
				</a>
			</div>
		</div>

		<!-- /Banner -->

		<!-- Novedades -->
		<div class="container products-wrapper">
			<div class="row">
				<div class="col-12">
					<h2 class="products-title">Más recientes</h2>
				</div>
				<!-- Show the "ultimos" products -->
				<% for( let i = 0; i < 4; i++ ) { %>
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="product/id/<%= productosNuevos[i].id %> ">
							<figure class="product-box_image">
								<img src="/images/products/<%= productosNuevos[i].cover %> "
									alt="<%= productosNuevos[i].title %> ">
							</figure>
							<article class="product-box_data">
								<h2><%= productosNuevos[i].title %></h2>
								<p><%= productosNuevos[i].author %></p>
								<p>Comentarios: <%= productosNuevos[i].comentarios.length %> </p>
								<p><a
									href="/profile/id/<%= productosNuevos[i].usuario.id %> "> Creado por:<%= productosNuevos[i].usuario.name %>
									<%= productosNuevos[i].usuario.last_name %> </a></p>
							</article>
						</a>
					</section>
				</div>
				<% } %>

			</div>
		</div>
		<!-- /Novedades -->
		<!-- Best Sellers -->
		<div class="container products-wrapper">
			<div class="row">
				<div class="col-12">
					<h2 class="products-title">Más Antiguos</h2>

				</div>
				<% for( let i = 0; i < 4; i++ ) { %>
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="product/id/<%= productosViejos[i].id %> ">
							<figure class="product-box_image">
								<img src="/images/products/<%= productosViejos[i].cover %> "
									alt="<%= productosViejos[i].title %> ">
							</figure>
							<article class="product-box_data">
								<h2><%= productosViejos[i].title %></h2>
								<p><%= productosViejos[i].author %></p>
								<p>Comentarios: <%= productosViejos[i].comentarios.length %> </p>
								<p><a
										href="/profile/id/<%= productosViejos[i].usuario.id %> "> Creado por:<%= productosViejos[i].usuario.name %>
										<%= productosViejos[i].usuario.last_name %> </a></p>
							</article>
						</a>
					</section>
				</div>
				<% } %>

			</div>
		</div>


		<!-- footer -->
		<%- include('./partials/footer')%>
		<!-- /footer -->

		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js"
			integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous">
		</script>

		<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.1/dist/umd/popper.min.js"
			integrity="sha384-SR1sx49pcuLnqZUnnPwx6FCym0wLsk5JZuNx2bPPENzswTNFaQU1RDvt3wT4gWFG" crossorigin="anonymous">
		</script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.min.js"
			integrity="sha384-j0CNLUeiqtyaRmlzUHCPZ+Gy5fQu0dQ6eZ/xAww941Ai1SxSY+0EQqNXNE6DZiVc" crossorigin="anonymous">
		</script>
	</body>

	</html>